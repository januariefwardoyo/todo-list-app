generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Todos {
  id                    String                @id                  @default(uuid())
  name_todo             String                
  status                Boolean               @default(false)
  price                 Int
  created_at            DateTime              @default(now())
  update_at             DateTime              @updatedAt
}